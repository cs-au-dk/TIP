bar(x){
    return 4;
}

unnormalized(){
    var foo;
    var x;
    var d;
    
    d = *x;
    *d = (*foo)(&x,bar(alloc null));
    
    return 10;
}


normalized(){
    var foo;
    var x;
    var a1,a2;
    var m;
    var f1,f2;
    var d;
    var r;
    
    f1 = bar;
    f2 = *foo;

    m = alloc null;
    
    a1 = &x;
    a2 = (f1)(m);
    d = *x;
    r = (f2)(a1,a2);
    *d = r;
    return 10;
}

